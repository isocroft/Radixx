  "contributors": [
    "@synergixe"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isocroft/Radixx"
  },
  "homepage": "https://github.com/isocroft/Radixx",
  "sideEffects": false,
  "devDependencies": {
    "jshint": "^2.9.4",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.8.0",
    "jasmine-core": "^2.2.0",
    "karma": "^0.12.31",
    "karma-firefox-launcher": "^0.1.4",
    "karma-jasmine": "^0.3.5",
    "benchmark": "~1.0.0",
    "babel-cli": "^6.26.",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
