    "babel-plugin-add-module-exports": "^0.2.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "yargs": "^10.0.3"
  },
  "dependencies": {},
  "engines": {
      "node": ">= 8.5.0"
  },
  "main": "src/radixx.js",
